<yas>
	<scene root="root" axis_length="3.0"/>

	<views default="presp0">
		<perspective id="presp0" near="0.1" far="500" angle="76">
			<from x="0.14" y="0.15" z="0.3" />
            <to x="2" y="0.7" z="1" />
		</perspective>

		<perspective id="presp1" near="0.1" far="500" angle="76">
            <from x="1.5" y="1" z="1.5" />
            <to x="1" y="0" z="1" />
        </perspective>

		<perspective id="presp2" near="0.1" far="500" angle="76">
			<from x="0.7" y="1" z="-0.7" />
            <to x="1" y="0" z="1" />
		</perspective>

		<perspective id="presp3" near="0.1" far="500" angle="76">
			<from x="-1.5" y="1" z="-1.5" />
            <to x="1" y="0" z="1" />
		</perspective>

		<perspective id="presp4" near="0.1" far="500" angle="90">
			<from x="1.5" y="1" z="1.5"/>
			<to x="1" y="0.0" z="1"/>
		</perspective>

		<ortho id="ortho0" near="0.1" far="500" left="-15" right="15" top="5" bottom="-7">
			<from x="13.0" y="6.0" z="50.0"/>
			<to x="13.0" y="6.0" z="0.0"/>
		</ortho>

		<ortho id="ortho1" near="0.1" far="500" left="-15" right="15" top="5" bottom="-7">
			<from x="50.0" y="6.0" z="13.0"/>
			<to x="0.0" y="6.0" z="13.0"/>
		</ortho>
	</views>

	<ambient>
		<ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
		<background r="0.1" g="0.2" b="0.3" a="0.4"/>
	</ambient>

	<lights>
		<omni id="omni0" enabled="1">
			<location x="4" y="10" z="5" w="1"/>
			<ambient r="0.1" g="0.1" b="0.1" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0.5" g="0.5" b="0.5" a="1"/>
		</omni>

		<omni id="omni1" enabled="1">
			<location x="21" y="10" z="5" w="1"/>
			<ambient r="0.1" g="0.1" b="0.1" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0.5" g="0.5" b="0.5" a="1"/>
		</omni>

		<omni id="omni2" enabled="1">
			<location x="4" y="10" z="21" w="1"/>
			<ambient r="0.1" g="0.1" b="0.1" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0.5" g="0.5" b="0.5" a="1"/>
		</omni>

		<omni id="omni3" enabled="1">
			<location x="21" y="10" z="21" w="1"/>
			<ambient r="0.1" g="0.1" b="0.1" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0.5" g="0.5" b="0.5" a="1"/>
		</omni>

		<spot id="spot0" enabled="0" angle="15" exponent="50">
			<location x="5" y="5" z="18" w="1"/>
			<target x="0" y="2" z="18"/>
			<ambient r="0" g="0" b="0" a="0"/>
			<diffuse r="0.5" g="0.5" b="0.5" a="0.5"/>
			<specular r="0.5" g="0.5" b="0.5" a="0.5"/>
		</spot>
	</lights>

	<textures>
		<texture id="wood" file="scenes/images/wood.jpg"/>
		<texture id="life-buoy" file="scenes/images/lifebuoy.png"/>
		<texture id="metal" file="scenes/images/metal.jpg"/>
		<texture id="cup" file="scenes/images/cup.jpg"/>
		<texture id="ball" file="scenes/images/ball.jpg"/>
		<texture id="floor" file="scenes/images/floor.jpg"/>
		<texture id="wall" file="scenes/images/wall.jpg"/>
		<texture id="blackboard" file="scenes/images/blackboard.jpg"/>
		<texture id="paper-plane" file="scenes/images/paper-plane.jpg"/>
		<texture id="rocket-red" file="scenes/images/rocketRed.jpg"/>
		<texture id="terrain_t" file="scenes/images/downldoad.jpg" />
		<texture id="terrain_h" file="scenes/images/heightmap_128.jpg" />
        <texture id="water_t" file="scenes/images/download.jpg" />
		<texture id="water_h" file="scenes/images/downloadH.png" />
	</textures>

	<materials>
		<material id="mater0" shininess="1">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0.2" g="0.2" b="0.2" a="1"/>
			<diffuse r="0.5" g="0.5" b="0.5" a="1"/>
			<specular r="0.5" g="0.5" b="0.5" a="1"/>
		</material>

		<material id="mater1" shininess="1">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0.5" g="0.5" b="0.5" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0" g="0" b="0" a="1"/>
		</material>

		<material id="mater2" shininess="1">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0.7" g="0.7" b="0.7" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0.4" g="0.4" b="0.4" a="1"/>
		</material>

		<material id="mater3" shininess="1">
			<emission r="0" g="0" b="0" a="0"/>
			<ambient r="1" g="1" b="1" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="1" g="1" b="1" a="1"/>
		</material>

		<material id="blue" shininess="30">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0" g="0" b="0.1" a="1"/>
			<diffuse r="0.2" g="0.2" b="0.3" a="1"/>
			<specular r="0.2" g="0.2" b="0.3" a="1"/>
		</material>

		<material id="orange" shininess="30">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0.1" g="0" b="0" a="1"/>
			<diffuse r="0.4" g="0.3" b="0.2" a="1"/>
			<specular r="0.4" g="0.3" b="0.2" a="1"/>
		</material>

		<material id="red" shininess="30">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0.1" g="0.1" b="0.1" a="1"/>
			<diffuse r="0.2" g="0.05" b="0.05" a="1"/>
			<specular r="0.2" g="0.05" b="0.05" a="1"/>
		</material>
	</materials>

	<transformations>
		<transformation id="transf0">
			<translate x="0" y="0" z="0"/>
			<rotate axis="x" angle="0"/>
			<scale x="1" y="1" z="1"/>
		</transformation>

		<transformation id="transfMesa">
			<translate x="0" y="-5" z="0"/>
		</transformation>

		<transformation id="globalDescale">
			<scale x="0.3" y="0.3" z="0.3"/>
		</transformation>
	</transformations>

	<animations>
		<linear id="liftoff" span="7">
			<controlpoint xx="0" yy="0" zz="0" />
			<controlpoint xx="0" yy="0" zz="3" />
			<controlpoint xx="0" yy="0" zz="0" />
			<controlpoint xx="0" yy="0" zz="6" />
		</linear>
	</animations>

	<primitives>	
		<primitive id="vehicle">
			<vehicle />
		</primitive>

		<primitive id="terrain">
            <terrain idtexture="terrain_t" idheightmap="terrain_h" parts="50" heightscale="0.15"/>
        </primitive >

		<primitive id="water_prim">
            <water idtexture="water_t" idwavemap="water_h" parts="100" heightscale="0.05" texscale="2"/>
        </primitive >   
	</primitives >

	<components>
		<component id="root">
			<transformation>
				<transformationref id="globalDescale"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="mater0"/>
			</materials>

			<texture id="none" length_s="1.0" length_t="1.0"/>

			<children>
				<componentref id="vehicle"/>
				<componentref id="terrain"/>
				<componentref id="water"/>
			</children>
		</component>

		<component id="vehicle">
			<transformation>
				<scale x="0.02" y="0.02" z="0.02" />
				<translate x="60" y="23" z="70" />
				<rotate axis="x" angle="-90"/>
			</transformation>

			<animations>
				<animationref id="liftoff" />
			</animations>

			<materials>
				<material id="mater0"/>
				<material id="mater2"/>
				<material id="orange"/>
				<material id="blue"/>
				<material id="red"/>
			</materials>

			<texture id="rocket-red" length_s="1.0" length_t="1.0"/>

			<children>
				<primitiveref id="vehicle" />
			</children>
		</component>

		<component id="terrain">
            <transformation>
                <scale x="3" y="5" z="3" />
                <translate x="0.8" y="-0.0229" z="0.8" />
			</transformation>

			<animations>
            </animations>

            <materials>
                <material id="mater0" />
            </materials>

            <texture id="terrain_t" length_s="1" length_t="0.3" />

            <children>
                <primitiveref id="terrain" />
            </children>
        </component>
		
        <component id="water">
            <transformation>
				<scale x="5" y="3" z="5" />
				<translate x="0.5" y="-0.0229" z="0.5" />
			</transformation>

			<animations>
            </animations>

            <materials>
                <material id="mater0" />
            </materials>

            <texture id="water_t" length_s="1" length_t="0.3" />

            <children>
                <primitiveref id="water_prim" />
            </children>
        </component>
	</components>

</yas>

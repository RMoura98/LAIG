			</children>
		</component>

		<component id="table-legs">
			<transformation>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="metal" length_s="1.0" length_t="1.0"/>

			<children>
				<componentref id="front-left-leg"/>
				<componentref id="front-right-leg"/>
				<componentref id="back-left-leg"/>
				<componentref id="back-right-leg"/>
			</children>
		</component>

		<component id="front-left-leg">
			<transformation>
				<translate x="-1.6" y="0" z="0.75"/>
				<rotate axis="x" angle="270"/>
				<scale x="1.0" y="1.0" z="1.0"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="inherit" length_s="1.0" length_t="1.0"/>

			<children>
				<primitiveref id="cylinder-leg"/>
			</children>
		</component>

		<component id="front-right-leg">
			<transformation>
				<translate x="1.6" y="0" z="0.75"/>
				<rotate axis="x" angle="270"/>
				<scale x="1.0" y="1.0" z="1.0"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="inherit" length_s="1.0" length_t="1.0"/>

			<children>
				<primitiveref id="cylinder-leg"/>
			</children>
		</component>

		<component id="back-left-leg">
			<transformation>
				<translate x="-1.6" y="0" z="-0.75"/>
				<rotate axis="x" angle="270"/>
				<scale x="1.0" y="1.0" z="1.0"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="inherit" length_s="1.0" length_t="1.0"/>

			<children>
				<primitiveref id="cylinder-leg"/>
			</children>
		</component>

		<component id="back-right-leg">
			<transformation>
				<translate x="1.6" y="0" z="-0.75"/>
				<rotate axis="x" angle="270"/>
				<scale x="1.0" y="1.0" z="1.0"/>

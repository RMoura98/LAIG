<yas>
	<scene root="root" axis_length="3.0"/>

	<views default="presp0">
		<perspective id="presp0" near="0.1" far="500" angle="76">
			<from x="2.5" y="2" z="6" />
            <to x="2.5" y="0" z="1" />
		</perspective>

		<perspective id="presp1" near="0.1" far="500" angle="76">
            <from x="6" y="1" z="0" />
            <to x="0" y="0" z="4" />
        </perspective>

		<perspective id="presp2" near="0.1" far="500" angle="76">
			<from x="0.1" y="1" z="6" />
            <to x="4" y="0" z="0" />
		</perspective>

		<perspective id="presp3" near="0.1" far="500" angle="76">
			<from x="4.5" y="3" z="4.5" />
            <to x="1.5" y="0" z="1.5" />
		</perspective>

		<perspective id="presp4" near="0.1" far="500" angle="90">
			<from x="6" y="2" z="10"/>
			<to x="4" y="0.0" z="8"/>
		</perspective>

		<ortho id="ortho0" near="0.1" far="500" left="-14" right="3" top="10" bottom="-8">
			<from x="5.0" y="6.0" z="10.0"/>
			<to x="13.0" y="6.0" z="0.0"/>
		</ortho>

		<ortho id="ortho1" near="0.1" far="500" left="-15" right="15" top="5" bottom="-7">
			<from x="50.0" y="6.0" z="13.0"/>
			<to x="0.0" y="6.0" z="13.0"/>
		</ortho>
	</views>

	<ambient>
		<ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
		<background r="0.1" g="0.2" b="0.3" a="0.4"/>
	</ambient>

	<lights>
		<omni id="omni0" enabled="1">
			<location x="4" y="10" z="5" w="1"/>
			<ambient r="0.1" g="0.1" b="0.1" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0.5" g="0.5" b="0.5" a="1"/>
		</omni>

		<omni id="omni1" enabled="1">
			<location x="21" y="10" z="5" w="1"/>
			<ambient r="0.1" g="0.1" b="0.1" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0.5" g="0.5" b="0.5" a="1"/>
		</omni>

		<omni id="omni2" enabled="1">
			<location x="4" y="10" z="21" w="1"/>
			<ambient r="0.1" g="0.1" b="0.1" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0.5" g="0.5" b="0.5" a="1"/>
		</omni>

		<omni id="omni3" enabled="1">
			<location x="21" y="10" z="21" w="1"/>
			<ambient r="0.1" g="0.1" b="0.1" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0.5" g="0.5" b="0.5" a="1"/>
		</omni>

		<spot id="spot0" enabled="0" angle="15" exponent="50">
			<location x="5" y="5" z="18" w="1"/>
			<target x="0" y="2" z="18"/>
			<ambient r="0" g="0" b="0" a="0"/>
			<diffuse r="0.5" g="0.5" b="0.5" a="0.5"/>
			<specular r="0.5" g="0.5" b="0.5" a="0.5"/>
		</spot>
	</lights>

	<textures>
		<texture id="wood" file="scenes/images/wood.jpg"/>
		<texture id="life-buoy" file="scenes/images/lifebuoy.png"/>
		<texture id="metal" file="scenes/images/metal.jpg"/>
		<texture id="cup" file="scenes/images/cup.jpg"/>
		<texture id="ball" file="scenes/images/ball.jpg"/>
		<texture id="floor" file="scenes/images/floor.jpg"/>
		<texture id="wall" file="scenes/images/wall.jpg"/>
		<texture id="blackboard" file="scenes/images/blackboard.jpg"/>
		<texture id="paper-plane" file="scenes/images/paper-plane.jpg"/>
		<texture id="rocket-red" file="scenes/images/rocketRed.jpg"/>
		<texture id="example" file="scenes/images/example.jpg"/>
		<texture id="terrain_t" file="scenes/images/downldoad.jpg" />
		<texture id="terrain_h" file="scenes/images/heightmap_128.jpg" />
        <texture id="water_t" file="scenes/images/download.jpg" />
		<texture id="water_h" file="scenes/images/downloadH.png" />
	</textures>

	<materials>
		<material id="mater0" shininess="1">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0.2" g="0.2" b="0.2" a="1"/>
			<diffuse r="0.5" g="0.5" b="0.5" a="1"/>
			<specular r="0.5" g="0.5" b="0.5" a="1"/>
		</material>

		<material id="mater1" shininess="1">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0.5" g="0.5" b="0.5" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0" g="0" b="0" a="1"/>
		</material>

		<material id="mater2" shininess="1">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0.7" g="0.7" b="0.7" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="0.4" g="0.4" b="0.4" a="1"/>
		</material>

		<material id="mater3" shininess="1">
			<emission r="0" g="0" b="0" a="0"/>
			<ambient r="1" g="1" b="1" a="1"/>
			<diffuse r="1" g="1" b="1" a="1"/>
			<specular r="1" g="1" b="1" a="1"/>
		</material>

		<material id="blue" shininess="30">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0" g="0" b="0.1" a="1"/>
			<diffuse r="0.2" g="0.2" b="0.3" a="1"/>
			<specular r="0.2" g="0.2" b="0.3" a="1"/>
		</material>

		<material id="orange" shininess="30">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0.1" g="0" b="0" a="1"/>
			<diffuse r="0.4" g="0.3" b="0.2" a="1"/>
			<specular r="0.4" g="0.3" b="0.2" a="1"/>
		</material>

		<material id="red" shininess="30">
			<emission r="0" g="0" b="0" a="1"/>
			<ambient r="0.1" g="0.1" b="0.1" a="1"/>
			<diffuse r="0.2" g="0.05" b="0.05" a="1"/>
			<specular r="0.2" g="0.05" b="0.05" a="1"/>
		</material>
	</materials>

	<transformations>
		<transformation id="transf0">
			<translate x="0" y="0" z="0"/>
			<rotate axis="x" angle="0"/>
			<scale x="1" y="1" z="1"/>
		</transformation>

		<transformation id="transfMesa">
			<translate x="0" y="-5" z="0"/>
		</transformation>

		<transformation id="globalDescale">
			<scale x="0.3" y="0.3" z="0.3"/>
		</transformation>
	</transformations>

	<animations>
		<linear id="arrival1" span="4">
			<controlpoint xx="0" yy="0" zz="0" />
			<controlpoint xx="3" yy="0" zz="0" />
		</linear>

		<circular id="arrivalRotation1" span="0.5" center="0 0 0" radius="0.01" startang="0" rotang="90" />

		<linear id="arrival2" span="4">
			<controlpoint xx="0" yy="0" zz="0" />
			<controlpoint xx="0" yy="0" zz="3" />
		</linear>

		<circular id="arrivalRotation2" span="0.5" center="0 0 0" radius="0.01" startang="0" rotang="-90" />

		<linear id="arrival3" span="3">
			<controlpoint xx="0" yy="0" zz="0" />
			<controlpoint xx="0" yy="0" zz="2" />
		</linear>

		<circular id="arrivalRotation3" span="0.5" center="0 0 0" radius="0.01" startang="0" rotang="-90" />

		<linear id="arrival4" span="3">
			<controlpoint xx="0" yy="0" zz="0" />
			<controlpoint xx="0" yy="0" zz="3" />
		</linear>

		<circular id="arrivalRotation4" span="0.5" center="0 0 0" radius="0.01" startang="0" rotang="90" />

		<linear id="arrival5" span="3">
			<controlpoint xx="0" yy="0" zz="0" />
			<controlpoint xx="0" yy="0" zz="3" />
		</linear>

		<circular id="rotation" span="30" center="0 0 -0.7" radius="1" startang="-90" rotang="-360" />

		<linear id="blastoff" span="5">
			<controlpoint xx="0" yy="0" zz="0" />
			<controlpoint xx="0" yy="0" zz="8" />
		</linear>
	</animations>

	<primitives>	
		<primitive id="quad">
			<rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5"/>
		</primitive>

		<primitive id="cylinder-leg">
			<cylinder base="0.16" top="0.16" height="1.5" slices="30" stacks="30"/>
		</primitive>  

		<primitive id="board">
			<board />
		</primitive>
	</primitives >

	<components>
		<component id="root">
			<transformation>
				<transformationref id="globalDescale"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="mater0"/>
			</materials>

			<texture id="none" length_s="1.0" length_t="1.0"/>

			<children>
				<componentref id="room"/>
			</children>
		</component>

		<component id="room">
			<transformation>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="mater0"/>
			</materials>

			<texture id="none" length_s="1.0" length_t="1.0"/>

			<children>
				<componentref id="table"/>
			</children>
		</component>

		<component id="table">
			<transformation>
				<translate x="2" y="0" z="2"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="mater0"/>
			</materials>

			<texture id="none"/>

			<children>
				<componentref id="top" />
				<componentref id="table-legs" />
				<primitiveref id="board" />
			</children>
		</component>

		<component id="top">
			<transformation>
				<translate x="0.0" y="1.60" z="0.0"/>
				<scale x="4.0" y="0.2" z="2.3"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="wood" length_s="1" length_t="1"/>

			<children>
				<componentref id="cube"/>
			</children>
		</component>

		<component id="cube">
			<transformation>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="inherit"/>

			<children>
				<componentref id="quad-front"/>
				<componentref id="quad-back"/>
				<componentref id="quad-left"/>
				<componentref id="quad-right"/>
				<componentref id="quad-top"/>
				<componentref id="quad-bot"/>
			</children>
		</component>

		<component id="quad-front">
			<transformation>
				<translate x="0.0" y="0.0" z="0.5"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="inherit" length_s="1" length_t="7"/>

			<children>
				<primitiveref id="quad"/>
			</children>
		</component>

		<component id="quad-back">
			<transformation>
				<translate x="0.0" y="0.0" z="-0.5"/>
				<rotate axis="y" angle="180"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="inherit"/>

			<children>
				<primitiveref id="quad"/>
			</children>
		</component>

		<component id="quad-left">
			<transformation>
				<translate x="-0.5" y="0.0" z="0.0"/>
				<rotate axis="y" angle="270"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="inherit"/>

			<children>
				<primitiveref id="quad"/>
			</children>
		</component>

		<component id="quad-right">
			<transformation>
				<translate x="0.5" y="0.0" z="0.0"/>
				<rotate axis="y" angle="90"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="inherit"/>

			<children>
				<primitiveref id="quad"/>
			</children>
		</component>

		<component id="quad-top">
			<transformation>
				<translate x="0.0" y="0.5" z="0.0"/>
				<rotate axis="x" angle="270"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="inherit"/>

			<children>
				<primitiveref id="quad"/>
			</children>
		</component>

		<component id="quad-bot">
			<transformation>
				<translate x="0.0" y="-0.5" z="0.0"/>
				<rotate axis="x" angle="90"/>
			</transformation>

			<animations>
			</animations>

			<materials>
				<material id="inherit"/>
			</materials>

			<texture id="inherit"/>

			<children>
